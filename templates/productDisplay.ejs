                $('#common_overlay, .popupInner').fadeOut(1000);
                $('#common_overlay').hide();
                $('body').css('position', 'relative');
                $('.popupModel > div').hide();
                $('.srch_sugg_lst').hide();
                $('#common_overlay').css('z-index', 1005);
            }else{
                e.stopPropagation();
            }
        }catch(e){
        }
    });
    $('.pincodeModel').on('click',function(e){
        e.stopPropagation();
    });
    $('.pincodeModel').on('click','a.close',function(){
        $('.popupModel > div').hide();
        $('#common_overlay').fadeOut(1000);
    });
</script>


